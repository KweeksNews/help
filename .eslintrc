{
  "extends": [
    "eslint:recommended",
    "plugin:@docusaurus/recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "JSX": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@docusaurus"
  ],
  "rules": {
    "import/no-unresolved": 0
  }
}
